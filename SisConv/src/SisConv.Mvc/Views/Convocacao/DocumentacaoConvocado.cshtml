@{
	ViewBag.Title = "DocumentacaoConvocado";
}

<div class="container">
	<div class="form-horizontal">
		<h4></h4>
		<h4></h4>
		<div class="jumbotron">
			<h1>@ViewBag.dadosConvocado.Nome</h1>
			<p>@ViewBag.dadosProcesso.TextoDeAceitacaoDaConvocacao</p>
			<p>Lista de Documentos para apresentação.</p>
			@foreach (var itens in ViewBag.ListaDocumentacao)
			{
				<p>@itens.Descricao</p>
			}
			<p>
				@Html.ActionLink("Atualizar Dados Cadastrais","Edit","Convocado",new{@id=ViewBag.dadosConvocado.ConvocadoId },null)
			</p>
		</div>
	</div>
</div>


